{
  "name": "rentomojo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-server": "nodemon --ignore 'back-end/' --ignore 'src/build/' server.js & node tasks/return.js",
    "open-browser": "nodemon --watch tasks/start-browser.js tasks/start-browser.js",
    "prettier-watch": "onchange -v '**/*.js' -- prettier --write file {{changed}}",
    "start-backend": "nodemon --watch back-end back-end/index.js & node tasks/return.js",
    "build-css": "node-sass src/index.sass -o src/build/styles/ -r --output-style compressed",
    "watch-css": "onchange -i -v 'src/**/*.sass' -- node tasks/return.js & npm run build-css",
    "inject": "postbuild -i src/index.html -o src/build/index.html -c src/build/styles -j src/build/libs",
    "watch-inject": "onchange -i 'src/build/libs/*' -e 'src/build/js/*' -- node tasks/return.js & npm run inject",
    "build-js": "copyfiles -V -e 'src/build/**/*.js' -f 'src/**/*.js' src/build/js",
    "build-libs": "node tasks/build-libs.js",
    "watch-build-libs": "onchange -i 'tasks/build-libs.js' -- node tasks/return.js & npm run build-libs",
    "watch-js": "onchange -i 'src/**/*.js' -e 'src/build/*' -- node tasks/return.js & npm run build-js",
    "babel": "babel 'src/build/js/*.js' --out-dir src/build/libs",
    "watch-babel": "onchange -i 'src/**/*.js' -e 'src/build/**/*.*' -- node tasks/return.js & npm run babel",
    "start-dev": "npm-run-all watch-css watch-js watch-build-libs watch-babel watch-inject start-server start-backend open-browser",
    "start": "node tasks/start-scripts.js && npm run start-dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/vue": "^2.0.0",
    "body-parser": "^1.19.0",
    "copy": "^0.3.2",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "cp-file": "^7.0.0",
    "del": "^5.1.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "make-dir": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.3",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "opn": "^6.0.0",
    "path": "^0.12.7",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "postbuild": "^2.1.0",
    "prettier": "1.18.2"
  }
}
